<section id="strive">
    <div class="container">
        <div class="title-section white">
            <h1>@Resource.StriveTo</h1>
            @ContentHelper.GetContent(Model.PageContents, EnumContentKey.StriveSubTitle)
        </div>

        <div   class="flex-strev">
            <div class="wow flipInY" data-wow-delay="0.1s">
                <span class="orange-line"></span>
                @ContentHelper.GetContent(Model.PageContents, EnumContentKey.StriveText1)
            </div>
            <div class="wow flipInX" data-wow-delay="0.5s">
                <span class="orange-line"></span>
                @ContentHelper.GetContent(Model.PageContents, EnumContentKey.StriveText2)
            </div>
            <div class="wow flipInY" data-wow-delay="0.9s">
                <span class="orange-line"></span>
                @ContentHelper.GetContent(Model.PageContents, EnumContentKey.StriveText3)
            </div>
            <div class="wow flipInX" data-wow-delay="1.4s">
                <span class="orange-line"></span>
                @ContentHelper.GetContent(Model.PageContents, EnumContentKey.StriveText4)
            </div>
        </div>
    </div>
</section>
