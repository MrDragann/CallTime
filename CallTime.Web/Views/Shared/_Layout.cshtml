<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>@ViewBag.Title</title>
    <link href='https://fonts.googleapis.com/css?family=Roboto:400,300,700,500' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="~/asserts/css/normalize.css"> <!-- CSS reset -->
    <link rel="stylesheet" href="~/asserts/css/bootstrap.min.css"> <!-- Bootstrap Grid -->
    <link rel="stylesheet" href="https://cdn.linearicons.com/free/1.0.0/icon-font.min.css">
    <link href="~/asserts/css/animate.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/asserts/css/style.css"> <!-- Resource style -->
    <link rel="stylesheet" href="~/asserts/css/magnific-popup.css"> <!-- Resource style -->
    <link href="~/asserts/css/main.css" rel="stylesheet" />
    <link href="~/fonts/fonts.css" rel="stylesheet" />
    <link href="~/asserts/css/demo.css" rel="stylesheet" />
    <link href="~/asserts/css/slick.css" rel="stylesheet" />
    <link rel="apple-touch-icon" sizes="180x180" href="~/asserts/img/favicon/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/asserts/img/favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/asserts/img/favicon/favicon-16x16.png">
    <link rel="manifest" href="~/asserts/img/favicon/site.webmanifest">
    <link href="~/asserts/css/magnific.css" rel="stylesheet" />
    <link rel="mask-icon" href="~/asserts/img/favicon/safari-pinned-tab.svg" color="#5bbad5">
    <link href="~/asserts/css/style-form.css" rel="stylesheet" />
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>

    @Html.Partial("_Header")
    @RenderBody()
    @Html.Partial("_Footer")


    <script src="~/asserts/js/jquery-2.1.4.min.js"></script> <!-- jQuery -->
    <script src="~/asserts/js/typed.js"></script>
    <script src="~/asserts/js/slick.js"></script>
    <script src="~/asserts/js/bootstrap.min.js"></script>  <!-- Bootstrap -->
    <script src="~/asserts/js/jquery-inview.js"></script>
    <script src="~/asserts/js/wow.min.js"></script>  <!-- wow -->
    <script src="~/asserts/js/jquery.magnific-popup.min.js"></script>  <!-- wow -->

    <script src="~/asserts/js/main.js"></script>  <!-- Main Script -->
    <script src="~/asserts/js/animate.js"></script>
    <script src="~/asserts/js/magnitf.js"></script>
    @RenderSection("scripts", required: false)
    <script>
        $(document).ready(function () {
            $('.slick-header').slick({
                dots: true,
                dotsClass: "my-dots",
                infinite: true,
                speed: 300,
                slidesToShow: 1,
                adaptiveHeight: true,
                arrows: false,
                autoplay: true,
                autoplaySpeed: 2500
            });

        });
    </script>

</body>
</html>
